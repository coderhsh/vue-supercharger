{
  "name": "vue-supercharger",
  "displayName": "Vue Supercharger",
  "publisher": "coderhsh",
  "description": "development tool tailored for Vue developers",
  "version": "1.0.3",
  "engines": {
    "vscode": "^1.80.0"
  },
  "keywords": [
    "vue",
    "vue2",
    "vue3",
    "vuejs",
    "vue.js",
    "composition-api",
    "composition",
    "html",
    "javascript",
    "snippet"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/coderhsh/vue-supercharger"
  },
  "bugs": {
    "url": "https://github.com/coderhsh/vue-supercharger/issues"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "icon": "assets/logo.jpeg",
  "categories": [
    "Snippets",
    "Other"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "vue",
        "extensions": [
          ".vue"
        ],
        "configuration": "./languages/vue-language-configuration.json"
      },
      {
        "id": "markdown",
        "configuration": "./languages/markdown-language-configuration.json"
      },
      {
        "id": "html",
        "configuration": "./languages/sfc-template-language-configuration.json"
      },
      {
        "id": "jade",
        "configuration": "./languages/sfc-template-language-configuration.json"
      },
      {
        "id": "plaintext",
        "configuration": "./languages/sfc-template-language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "vue",
        "scopeName": "source.vue",
        "path": "./syntaxes/vue.tmLanguage.json",
        "embeddedLanguages": {
          "source.vue": "vue",
          "text": "plaintext",
          "text.html.derivative": "html",
          "text.html.markdown": "markdown",
          "text.pug": "jade",
          "source.css": "css",
          "source.css.scss": "scss",
          "source.css.less": "less",
          "source.sass": "sass",
          "source.stylus": "stylus",
          "source.postcss": "postcss",
          "source.js": "javascript",
          "source.ts": "typescript",
          "source.js.jsx": "javascriptreact",
          "source.tsx": "typescriptreact",
          "source.coffee": "coffeescript",
          "meta.tag.js": "jsx-tags",
          "meta.tag.tsx": "jsx-tags",
          "meta.tag.without-attributes.js": "jsx-tags",
          "meta.tag.without-attributes.tsx": "jsx-tags",
          "source.json": "json",
          "source.json.comments": "jsonc",
          "source.json5": "json5",
          "source.yaml": "yaml",
          "source.toml": "toml",
          "source.graphql": "graphql"
        },
        "unbalancedBracketScopes": [
          "keyword.operator.relational",
          "storage.type.function.arrow",
          "keyword.operator.bitwise.shift",
          "meta.brace.angle",
          "punctuation.definition.tag"
        ]
      },
      {
        "scopeName": "markdown.vue.codeblock",
        "path": "./syntaxes/markdown-vue.json",
        "injectTo": [
          "text.html.markdown"
        ],
        "embeddedLanguages": {
          "meta.embedded.block.vue": "vue",
          "source.vue": "vue",
          "text": "plaintext",
          "text.html.derivative": "html",
          "text.html.markdown": "markdown",
          "text.pug": "jade",
          "source.css": "css",
          "source.css.scss": "scss",
          "source.css.less": "less",
          "source.sass": "sass",
          "source.stylus": "stylus",
          "source.postcss": "postcss",
          "source.js": "javascript",
          "source.ts": "typescript",
          "source.js.jsx": "javascriptreact",
          "source.tsx": "typescriptreact",
          "source.coffee": "coffeescript",
          "meta.tag.js": "jsx-tags",
          "meta.tag.tsx": "jsx-tags",
          "meta.tag.without-attributes.js": "jsx-tags",
          "meta.tag.without-attributes.tsx": "jsx-tags",
          "source.json": "json",
          "source.json.comments": "jsonc",
          "source.json5": "json5",
          "source.yaml": "yaml",
          "source.toml": "toml",
          "source.graphql": "graphql"
        }
      },
      {
        "scopeName": "mdx.vue.codeblock",
        "path": "./syntaxes/mdx-vue.json",
        "injectTo": [
          "source.mdx"
        ],
        "embeddedLanguages": {
          "mdx.embedded.vue": "vue",
          "source.vue": "vue",
          "text": "plaintext",
          "text.html.derivative": "html",
          "text.html.markdown": "markdown",
          "text.pug": "jade",
          "source.css": "css",
          "source.css.scss": "scss",
          "source.css.less": "less",
          "source.sass": "sass",
          "source.stylus": "stylus",
          "source.postcss": "postcss",
          "source.js": "javascript",
          "source.ts": "typescript",
          "source.js.jsx": "javascriptreact",
          "source.tsx": "typescriptreact",
          "source.coffee": "coffeescript",
          "meta.tag.js": "jsx-tags",
          "meta.tag.tsx": "jsx-tags",
          "meta.tag.without-attributes.js": "jsx-tags",
          "meta.tag.without-attributes.tsx": "jsx-tags",
          "source.json": "json",
          "source.json.comments": "jsonc",
          "source.json5": "json5",
          "source.yaml": "yaml",
          "source.toml": "toml",
          "source.graphql": "graphql"
        }
      },
      {
        "scopeName": "vue.directives",
        "path": "./syntaxes/vue-directives.json",
        "injectTo": [
          "source.vue",
          "text.html.markdown",
          "text.html.derivative",
          "text.pug"
        ]
      },
      {
        "scopeName": "vue.interpolations",
        "path": "./syntaxes/vue-interpolations.json",
        "injectTo": [
          "source.vue",
          "text.html.markdown",
          "text.html.derivative",
          "text.pug"
        ]
      },
      {
        "scopeName": "vue.sfc.style.variable.injection",
        "path": "./syntaxes/vue-sfc-style-variable-injection.json",
        "injectTo": [
          "source.vue"
        ]
      }
    ],
    "breakpoints": [
      {
        "language": "vue"
      }
    ],
    "configuration": {
      "title": "Vue Supercharger",
      "properties": {
        "vue-supercharger.vueSelection": {
          "order": 0,
          "type": "string",
          "enum": [
            "vue2",
            "vue3",
            "both",
            "auto",
            "none"
          ],
          "markdownEnumDescriptions": [
            "%configuration.vueSelection.vue2%",
            "%configuration.vueSelection.vue3%",
            "%configuration.vueSelection.both%",
            "%configuration.vueSelection.auto%",
            "%configuration.vueSelection.none%"
          ],
          "default": "auto",
          "description": "%configuration.vueSelection.description%"
        },
        "vue-supercharger.ignoreVueVersionMismatch": {
          "order": 20,
          "type": "boolean",
          "default": false,
          "description": "%configuration.ignoreVueVersionMismatch.description%"
        }
      }
    },
    "menus": {
      "editor/context": [],
      "explorer/context": []
    },
    "snippets": [
      {
        "language": "html",
        "path": "./snippets/vue-html.code-snippets"
      }
    ],
    "commands": [
      {
        "command": "vue-supercharger.selectVueVersion",
        "title": "%command.selectVueVersion.title%"
      }
    ]
  },
  "files": [
    "out",
    "snippets",
    "snippetFiles",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "build": "vsce package",
    "publish": "vsce publish",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/mocha": "^10.0.9",
    "@types/node": "20.x",
    "@types/vscode": "^1.80.0",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.7.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.13.0",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "fs-extra": "^11.2.0"
  }
}
